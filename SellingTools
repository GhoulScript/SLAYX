Frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
Frame.BorderColor3 = Color3.fromRGB(25, 25, 25)
Frame.Position = UDim2.new(0.411347568, 0, 0.711111069, 0)
Frame.Size = UDim2.new(0, 248, 0, 110)
Frame_2.Parent = Frame
Frame_2.BackgroundColor3 = Color3.fromRGB(39, 39, 39)
Frame_2.BorderColor3 = Color3.fromRGB(39, 39, 39)
